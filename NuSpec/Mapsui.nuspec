<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>Mapsui</id>
    <version>$version$</version>
    <title>Mapsui</title>
    <authors>
      Paul den Dulk
      Wojciech Iliszko
    </authors>
    <owners>Wojciech Iliszko</owners>
    <projectUrl>https://github.com/liszko/Mapsui</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
