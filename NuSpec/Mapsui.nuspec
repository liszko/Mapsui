    <file src="..\Mapsui.Desktop\bin\Release\Mapsui.Desktop.dll" target="lib\net461\Mapsui.Desktop.dll" />
    <file src="..\Mapsui.UI.Wpf\bin\Release\Mapsui.Rendering.Xaml.dll" target="lib\net461\Mapsui.Rendering.Xaml.dll" />
